<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Suite">
  <test name="800 x 600 resolution">
    <parameter name="resolution" value="mobile"/>
    <classes>
      <class name="selenium.TestTask1" />
    </classes>
  </test>
  <test name="1024 x 768 resolution">
    <parameter name="resolution" value= "low"/>
    <classes>
      <class name="selenium.TestTask1" />
    </classes>
  </test>
  <test name="Maximum resolution">
    <parameter name="resolution" value="max"/>
    <classes>
      <class name="selenium.TestTask1" />
    </classes>
  </test>
</suite>